{
  "name": "aws-app",
  "description": "aws-app",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@aviene/core": "workspace:^",
    "@middy/core": "^5.4.1"
  },
  "devDependencies": {
    "@aviene/eslint-config-node": "latest",
    "@aviene/eslint-config-typescript": "latest",
    "@aviene/typedoc-config-markdown": "latest",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/node": "^20",
    "jest": "^29.2.1"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint ./src",
    "create:docs": "typedoc"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "**/__tests__/**/*.mjs?(x)",
      "**/?(*.)+(spec|test).mjs?(x)"
    ],
    "moduleFileExtensions": [
      "mjs",
      "js"
    ]
  }
}
