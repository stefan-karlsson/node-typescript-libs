{
  "name": "@aviene/common",
  "description": "Commonly used Typescript code",
  "version": "0.1.0",
  "author": {
    "name": "Stefan Karlsson",
    "email": "iam@stefan-karlsson.se",
    "url": "https://stefan-karlsson.se"
  },
  "bugs": {
    "url": "https://github.com/stefan-karlsson/node-typescript-libs/issues"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && vite build"
  },
  "dependencies": {
    "@aviene/ansi-escape": "workspace:^"
  },
  "devDependencies": {
    "typescript": "^5.4.3",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.8.1",
    "@tsconfig/node-lts": "latest",
    "@aviene/eslint-config-typescript": "latest",
    "@types/node": "^20"
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./logging": {
      "types": "./dist/logging.d.ts",
      "import": "./dist/logging.js",
      "require": "./dist/logging.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    }
  },
  "keywords": [
    "aviene",
    "logging"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/stefan-karlsson/node-typescript-libs.git"
  }
}
