{
  "name": "@aviene/common",
  "description": "Commonly used Typescript code",
  "version": "0.1.2",
  "author": {
    "name": "Stefan Karlsson",
    "email": "iam@stefan-karlsson.se",
    "url": "https://stefan-karlsson.se"
  },
  "bugs": {
    "url": "https://github.com/stefan-karlsson/node-typescript-libs/issues"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint ./src",
    "create:docs": "typedoc"
  },
  "dependencies": {
    "@aviene/ansi-escape": "workspace:^",
    "@aviene/common": "workspace:^"
  },
  "devDependencies": {
    "@aviene/typedoc-config-markdown": "latest",
    "@aviene/eslint-config-node": "latest",
    "@aviene/eslint-config-typescript": "latest",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/node": "^20.12.5",
    "typescript": "^5.4.4",
    "typedoc": "^0.25.13",
    "vite": "^5.3.2",
    "vite-plugin-dts": "^3.8.3"
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./logging": {
      "types": "./dist/logging.d.ts",
      "import": "./dist/logging.js",
      "require": "./dist/logging.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    }
  },
  "keywords": [
    "aviene",
    "logging",
    "types",
    "utils"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/stefan-karlsson/node-typescript-libs.git"
  }
}
