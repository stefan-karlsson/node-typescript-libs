{
  "name": "@aviene/exceptions",
  "description": "Provides a set of custom exception classes for handling errors in JavaScript applications",
  "version": "0.1.1",
  "author": {
    "name": "Stefan Karlsson",
    "email": "iam@stefan-karlsson.se",
    "url": "https://stefan-karlsson.se"
  },
  "bugs": {
    "url": "https://github.com/stefan-karlsson/node-typescript-libs/issues"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint ./src",
    "create:docs": "pnpm run --filter ./packages/** typedoc"
  },
  "devDependencies": {
    "@aviene/typedoc-config-markdown": "latest",
    "@aviene/eslint-config-node": "latest",
    "@aviene/eslint-config-typescript": "latest",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/node": "^20.12.5",
    "typescript": "^5.4.4",
    "typedoc": "^0.25.13",
    "vite": "^5.3.2",
    "vite-plugin-dts": "^3.8.3"
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./exceptions": {
      "types": "./dist/exceptions.d.ts",
      "import": "./dist/exceptions.js",
      "require": "./dist/exceptions.cjs"
    }
  },
  "keywords": [
    "ddd",
    "exceptions",
    "aviene"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/stefan-karlsson/node-typescript-libs.git"
  }
}